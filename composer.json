{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/M2Mobi/ApnsPHP.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/M2Mobi/php-resque.git"
    }
  ],
  "require": {
    "psr/log": "~1.1.0",
    "resque/php-resque": "dev-master#981ef3da53f671c14f8fc61a233c3974f04128a9",
    "duccio/apns-php": "dev-master#5c5a6d65b1c88a2acb1cab32e5efda7f5bd5988c",
    "phpmailer/phpmailer": "~6.0.0",
    "rmccue/requests": "~1.7.0",
    "colinmollenhour/credis": "~1.7",
    "php": ">=7.2",
    "psr/cache": "~1.0",
    "lunr/halo": "dev-master",
    "lunr/config": "dev-master"
  },
  "suggest": {
    "ext-http": "Needed for HTTP support in Corona, needed to get hostname in Shadow when parsing data",
    "ext-msgpack": "Needed for msgpack support in Corona",
    "ext-gettext": "Needed for internationalization support in L10n",
    "ext-mysqli": "Needed for MySQL/MariaDB support in Gravity (0.1)"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "ext-xdebug": "~2.6"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {"Lunr\\": "src/Lunr/"}
  }
}
