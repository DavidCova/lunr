    "PHP-Resque": {
        "url": "https://github.com/resque/php-resque.git",
        "version": "1.3.6",
        "psr0": {
            "path": "/lib/"
        }
    },
    "Psr-Cache": {
        "url": "https://github.com/php-fig/cache.git",
        "version": "1.0.1",
        "psr4": {
            "prefix": "Psr\\Cache",
            "search-path": "/src/"
        }
    }
